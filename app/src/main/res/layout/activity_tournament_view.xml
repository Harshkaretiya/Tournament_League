                android:layout_marginTop="15dp"
                android:backgroundTint="@color/white"
                app:cardCornerRadius="10dp"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:gravity="center"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:fontFamily="@font/airbnbcereal_bk"
                            android:textColor="@color/text_color_grey"
                            android:text="Entry Fee : "
                            />
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:cardCornerRadius="10dp"
                            app:strokeWidth="0dp"
                            android:layout_marginLeft="5dp"
                            android:backgroundTint="@color/border_color_grey"
                            >
                            <TextView
                                android:id="@+id/enterFee"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="200"
                                android:fontFamily="@font/airbnbcereal_md"
                                android:textSize="14sp"
                                android:textColor="@color/text_color_black"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="8dp"
                                />
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:gravity="center"
                        android:layout_marginTop="10dp"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:fontFamily="@font/airbnbcereal_bk"
                            android:textColor="@color/text_color_grey"
                            android:text="Your Coins : "
                            />
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            app:cardCornerRadius="10dp"
                            app:strokeWidth="0dp"
                            android:layout_marginLeft="5dp"
                            android:backgroundTint="#F0FF91"
                            >
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="10dp"
                                android:layout_marginVertical="5dp"
                                android:gravity="center"
                                >
                            <ImageView

                                android:id="@+id/cart_home_fragment"
                                android:layout_width="16dp"
                                android:layout_height="16dp"
                                android:src="@drawable/coins"
                                />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="100"
                                    android:textSize="14sp"
                                    android:fontFamily="@font/airbnbcereal_md"
                                    android:layout_marginLeft="5dp"
                                    android:textStyle="bold"
                                    android:textColor="@color/text_color_black"
                                    />
                            </LinearLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="10dp"
                        >
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="20dp"
                            app:strokeWidth="0dp"
                            app:cardCornerRadius="10dp"
                            android:backgroundTint="@color/blue_text"
                            android:layout_marginRight="10dp"
                            android:layout_weight="1"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Save Team"
                                android:fontFamily="@font/airbnbcereal_md"
                                android:textSize="16sp"
                                android:textColor="@color/white"
                                android:layout_marginVertical="15dp"
                                />
                        </com.google.android.material.card.MaterialCardView>
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="20dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            app:strokeWidth="0dp"
                            app:cardCornerRadius="10dp"
                            android:backgroundTint="@color/green_background_color"
                            android:layout_gravity="center"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="JOIN"
                                android:fontFamily="@font/airbnbcereal_md"
                                android:textSize="16sp"
                                android:textColor="@color/white"
                                android:layout_marginVertical="15dp"
                                />
                        </com.google.android.material.card.MaterialCardView>
                    </LinearLayout>
                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>